{
  "name": "Tshop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "iosx": "react-native run-ios --simulator 'iPhone X'",
    "format": "node_modules/eslint/bin/eslint.js . --fix"
  },
  "dependencies": {
    "babel-eslint": "^8.2.3",
    "jcore-react-native": "^1.2.5",
    "jpush-react-native": "^2.1.12",
    "mobx": "^3.6.1",
    "mobx-react": "^4.4.3",
    "react": "16.2.0",
    "react-native": "0.53.0",
    "react-native-fast-image": "^2.2.5",
    "react-native-root-toast": "^3.0.1",
    "react-native-scrollable-tab-view": "^0.8.0",
    "react-native-swiper": "^1.5.13",
    "react-native-vector-icons": "^4.6.0",
    "react-native-wechat": "^1.9.9",
    "react-navigation": "^1.1.2",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.19",
    "@types/react-navigation": "^1.2.3",
    "@types/react-test-renderer": "^16.0.1",
    "babel-jest": "22.2.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^4.19.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "22.2.2",
    "react-test-renderer": "16.2.0",
    "ts-jest": "^22.4.2",
    "typescript": "^2.7.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
